package swea;

import java.util.Scanner;

public class d4_1258 {
	static int[] dx = {0,1};
	static int[] dy = {1,0};
	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Scanner sc = new Scanner(System.in);
		int T = sc.nextInt();
		for(int t=1;t<=T;t++) {
			int N = sc.nextInt();
			int[][] a = new int[N][N];
			for(int i=0;i<N;i++) {
				for(int j=0;j<N;j++) {
					a[i][j]=sc.nextInt();
				}
			}
		}
				
		
	}

}
