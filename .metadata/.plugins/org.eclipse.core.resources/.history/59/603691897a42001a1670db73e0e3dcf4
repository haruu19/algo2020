public class Tower {
	static int N;
	static int[] a;
	static int[] b;
	public static void main(String[] args) throws FileNotFoundException {
		// TODO Auto-generated method stub
		Scanner sc = new Scanner(new FileInputStream("input.txt"));
		N = sc.nextInt();
		a = new int[N];
		b = new int[N];
		for(int i=0;i<N;i++)
			a[i] = sc.nextInt();
			
		
	}

}
