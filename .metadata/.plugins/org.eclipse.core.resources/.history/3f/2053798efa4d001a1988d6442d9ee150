import java.util.Scanner;

public class d4_4408 {
	static class Ref{
		int x;
		int y;
		Ref(int x, int y){
			this.x=x;
			this.y=y;
		}
	}
	
	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		int N = sc.nextInt();
		Ref[] ref = new Ref[N];
		int n1=0;
		int n2=0;
		for(int i=0;i<N;i++) {
			ref[i] = new Ref(sc.nextInt(),sc.nextInt());
		}
	}
}
