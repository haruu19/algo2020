import java.util.Scanner;

public class boj_7576 {
	static int M,N;
	static int[][] a;
	static int[] dx = {1,-1,0,0};
	static int[] dy = {0,0,1,-1};
	static boolean[][] cur;//현재 처리할 토마토 체크
	
	static class Tomato{
		int x;
		int y;
		Tomato(int x, int y){
			this.x = x;
			this.y = y;
		}
	}
	
	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		M = sc.nextInt();
		N = sc.nextInt();
		a = new int[N][M];
		cur = new boolean[N][M];
		for(int i=0;i<N;i++) {
			for(int j=0;j<M;j++) {
				a[i][j]=sc.nextInt();
				if(a[i][j]==1)
					cur[i][j]=true;
			}
		}
		int count = 0;
		while(true) {
			if(hasNoZero())
				break;
			for(int i=0;i<N;i++) {
				for(int j=0;j<M;j++) {
					if(a[i][j]==1&&cur[i][j]) {
						bfs(i,j);
						a[i][j]=2;
						cur[i][j]=false;
					}
				}
			}
			count++;
		}
		System.out.println(count);
	}
	
	static void bfs() {
		Queue
	}
	
	static boolean isOut(int x, int y) {
		return (x<0||x>=N||y<0||y>=M);
	}
	
	static boolean hasNoZero() {
		for(int i=0;i<N;i++) {
			for(int j=0;j<M;j++) {
				if(a[i][j]==0)
					return false;
			}
		}
		return true;
	}
}
