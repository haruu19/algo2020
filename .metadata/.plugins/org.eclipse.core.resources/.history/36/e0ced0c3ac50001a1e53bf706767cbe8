import java.util.Arrays;
import java.util.Scanner;

public class boj_17135 {
	static int[] dx = {0,-1,0};
	static int[] dy = {-1,0,1};
	static int[][] map;
	static int N,M,D;
	static int ans;
	static boolean[] archor;
	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Scanner sc = new Scanner(System.in);
		N = sc.nextInt();
		M = sc.nextInt();
		D = sc.nextInt();
		map = new int[N+1][M+1];
		for(int i=1;i<=N;i++) {
			for(int j=1;j<=M;j++) {
				map[i][j]=sc.nextInt();
			}
		}
		archor = new boolean[M+1];
		//Comb
		comb(0,0);
		//게임진행
		int cnt = 1;
		while(true) {
			if(isClear())
				break;
			//적을 죽인다
			kill();
			for(int i=N;i>=1;i--)
				map[i] = Arrays.copyOf(map[i-1], M+1);
			for(int i=1;i<=cnt;i++)
				Arrays.fill(map[i], 0);
			cnt++;
		}
		
		System.out.println(ans);
	}
	
	static void comb(int prev, int cnt) {
		if(cnt == 3) {
			play();
			return;
		}
		
		for(int i=1;i<=M;i++) {
			if(i<=prev)
				continue;
			archor[i] = true;
			comb(i,cnt+1);
			archor[i] = false;
		}
	}
	
	static void play() {
		
	}
	
	static void kill() {
		
	}
	
	static boolean isClear() {
		for(int i=1;i<=N;i++)
			for(int j=1;j<=M;j++)
				if(map[i][j]!=0)
					return false;
		return true;
	}
}
