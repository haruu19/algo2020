package swea;

import java.util.Scanner;

public class d4_1258 {
	static int[] dx = {0,1};
	static int[] dy = {1,0};
	static int[][] a;
	static boolean[][] v;
	static int N;
	static int height;
	static int weight;
	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Scanner sc = new Scanner(System.in);
		int T = sc.nextInt();
		for(int t=1;t<=T;t++) {
			N = sc.nextInt();
			a = new int[N][N];
			for(int i=0;i<N;i++) {
				for(int j=0;j<N;j++) {
					a[i][j]=sc.nextInt();
				}
			}
		}
		
		for(int i=0;i<N;i++) {
			for(int j=0;j<N;j++) {
				if(a[i][j]!=0&&!v[i][j]) {
					method(i,j);
				}
			}
		}
	}
	static void method(int x, int y) {
		int nx,ny;
		while(!isOut(x,y)&&x>0&&y>0) {
			nx = x + dx[0];
			ny = y + dy[0];
			
		}
	}
	static boolean isOut(int x, int y) {
		return (x<0||x>=N||y<0||y>=N);
	}
}
