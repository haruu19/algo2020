package day7;

public class C {
	public static void main(String[] args) {
		BST bst = new BST();
		bst.insert(9);
		bst.insert(4);
	}
	
	static class BST{
		Node root;
		void insert(int data) {
			Node temp = new Node(data);
			Node p = root;
			if(root==null) {
				root = temp;
				return;
			}
			while(p!=null) {
				if(temp.data<p.data) {
					p = p.left;
				}else {
					p = p.right;
				}
			}
			p = temp;
		}
	}
	static class Node{
		int data;
		Node left;
		Node right;
		public Node(int data) {
			super();
			this.data = data;
		}
	}
	
}
